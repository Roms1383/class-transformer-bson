{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2018",
    "lib": ["es2018"],
    "outDir": "dist",
    "rootDir": ".",
    "noImplicitAny": false,
    "strict": false,
    "strictPropertyInitialization": false,
    "sourceMap": false,
    "removeComments": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["node_modules"]
}